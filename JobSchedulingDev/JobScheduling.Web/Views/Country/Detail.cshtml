@model JobScheduling.Model.MasterModel.CountryM
@{
    ViewBag.Title = "Country Detail";
}

@section secFunctionBar{
    <div id="divContentFunction">
        <ul class="dark_color5">
            <li>
                <a href="/Country/List">
                    <span class="back">
                        <img alt="" src="/Content/Images/ico_common.png" />
                    </span>
                    Back
                </a>
            </li>
        </ul>
    </div>
}
@using (Html.BeginForm())
{
    <ul class="content_infoview View">
        <li>
            <h4>
               Code
            </h4>
            @Html.DisplayTextFor(m => m.Code)
        </li>
        <li>
            <h4>
               Description
            </h4>
            @Html.DisplayFor(m => m.Description)
        </li>
        
        <li>
            <input type="button" class="input_back" onclick="window.location.href = '/Country/List';" />
        </li>
    </ul>

}
