<ul class="content_infoview @ViewBag.Title">
    <li>
        <h4>Number</h4>
        <input type="text" name="Title" title="Title" value="20131014"/>
        <label class="errormessage need">* errormessage</label>
    </li>
    <li>
        <h4>Name</h4>
        <input type="text" name="Title" title="Title" value="Project Name"/>
        <label class="errormessage need">* errormessage</label>
    </li>
    <li>
        <h4>Description</h4>
        <input type="text" name="Title" title="Title" value="我和你吻别，在无人的街"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Status</h4>
        <input type="text" name="Title" title="Title" value="InProcess Step 1"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Begin Data</h4>
        <input type="text" name="Title" title="Title" value="2013/10/12" class="text_BeginDate" />
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>End Data</h4>
        <input type="text" name="Title" title="Title" value="@DateTime.Now.ToString("yyyy-m-d")" class="text_EndDate"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Project Manager</h4>
        <input type="text" name="Title" title="Title" value="Donny"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Project Master</h4>
        <input type="text" name="Title" title="Title" value="Donny"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Project Team</h4>
        @{
            for(int i =0;i<6;i++)
            {
                <b>帅哥 @i</b>
            }   
         }
    </li>
    <li>
        <h4>Doc Link</h4>
        <input type="text" name="Title" title="Title" value="http://www.我是传奇.com "/> 
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Cost</h4>
        <input type="text" name="Title" title="Title" value="10,000"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Paid</h4>
        <input type="text" name="Title" title="Title" value="400"/>
        <label class="errormessage">errormessage</label>
    </li>
    <li>
        <h4>Sum</h4>
        <input type="text" name="Title" title="Title" value="9,600"/>
        <label class="errormessage">errormessage</label>
    </li>
</ul>