    <script type="text/javascript">
        $(document).ready(function () {
            var ft_1 = 10;
            var ft_2 = 20;
            var ft_3 = 15;
            var ft_4 = 20;
            var ft_5 = 15;

            var pib_1 = 15;
            var pib_2 = 15;
            var pib_3 = 30;
            var pib_4 = 20;

            $(".div_projectLogBook_all").css("width", $(window).width() - 250);

            $(".textarea_detail").css({
                "max-height": "60px",
                "width": "66%",
                "resize": "none",
                "background-color" : "rgb(230, 238, 240) !Important"
            });

            $(".content_gridview_header").css({
                "width": "100%",
                "margin" : "2px 0 0 0"
            });

            $(".content_gridview_row").css({
                "width": "100%",
            });

            $(".content_gridview_header.FunctionDetail").each(function () {
                $(this).find("ul li").eq(0).css("width", ft_1 + "%");
                $(this).find("ul li").eq(1).css("width", ft_2 + "%");
                $(this).find("ul li").eq(2).css("width", ft_3 + "%");
                $(this).find("ul li").eq(3).css("width", ft_4 + "%");
                $(this).find("ul li").eq(4).css("width", ft_5 + "%");
            });

            $(".content_gridview_row.FunctionDetail").each(function () {
                $(this).find("ul li").eq(0).css("width", ft_1 + "%");
                $(this).find("ul li").eq(1).css("width", ft_2 + "%");
                $(this).find("ul li").eq(2).css("width", ft_3 + "%");
                $(this).find("ul li").eq(3).css("width", ft_4 + "%");
                $(this).find("ul li").eq(4).css("width", ft_5 + "%");
            });

            $(".content_gridview_header.ProjectIncomeBreakdown").each(function () {
                $(this).find("ul li").eq(0).css("width", pib_1 + "%");
                $(this).find("ul li").eq(1).css("width", pib_2 + "%");
                $(this).find("ul li").eq(2).css("width", pib_3 + "%");
                $(this).find("ul li").eq(3).css("width", pib_4 + "%");
            });

            $(".content_gridview_row.ProjectIncomeBreakdown").each(function () {
                $(this).find("ul li").eq(0).css("width", pib_1 + "%");
                $(this).find("ul li").eq(1).css("width", pib_2 + "%");
                $(this).find("ul li").eq(2).css("width", pib_3 + "%");
                $(this).find("ul li").eq(3).css("width", pib_4 + "%");
            });

            $(".content_gridview_header.BudgetCostDetail").each(function () {
                $(this).find("ul li").eq(0).css("width", pib_1 + "%");
                $(this).find("ul li").eq(1).css("width", pib_2 + "%");
                $(this).find("ul li").eq(2).css("width", pib_3 + "%");
                $(this).find("ul li").eq(3).css("width", pib_4 + "%");
            });

            $(".content_gridview_row.BudgetCostDetail").each(function () {
                $(this).find("ul li").eq(0).css("width", pib_1 + "%");
                $(this).find("ul li").eq(1).css("width", pib_2 + "%");
                $(this).find("ul li").eq(2).css("width", pib_3 + "%");
                $(this).find("ul li").eq(3).css("width", pib_4 + "%");
            });

            $(".div_paging").css("width", "100%");
        });

        function toggleContent(module) {
            $(".div_projectFunction_content." + module).toggle();
            if ($(".div_projectFunction_content." + module).css("display") == "none") {
                $(".div_projectFunction_hide." + module + " a").html("Show");
            } else {
                $(".div_projectFunction_hide." + module + " a").html("Hide");
            }
        }
    </script>
